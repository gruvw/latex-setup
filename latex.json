{
	"[G] Aligned": {
		"prefix": "ali",
		"body": [
			"\\begin{aligned}",
			"\t${1:$TM_SELECTED_TEXT}",
			"\\end{aligned}"
		]
	},
	"[G] Gathered": {
		"prefix": "gat",
		"body": [
			"\\begin{gathered}",
			"\t${1:$TM_SELECTED_TEXT}",
			"\\end{gathered}"
		]
	},
	"[G] Cases": {
		"prefix": "cas",
		"body": [
			"\\begin{cases}",
			"\t${1:$TM_SELECTED_TEXT}",
			"\\end{cases}"
		]
	},
	"[G] Enumerate": {
		"prefix": "enm",
		"body": [
			"\\begin{enumerate}",
			"\t\\item ${1:$TM_SELECTED_TEXT}",
			"\\end{enumerate}"
		]
	},
	"[G] Itemize": {
		"prefix": "itz",
		"body": [
			"\\begin{itemize}",
			"\t\\item ${1:$TM_SELECTED_TEXT}",
			"\\end{itemize}"
		]
	},
	"[G] Image": {
		"prefix": "img",
		"body": [
			"\\begin{center}",
			"\t\\includegraphics[width=${2:8}cm]{$1}",
			"\\end{center}",
			"$0"
		]
	},
	"[G] Add margin": {
		"prefix": "adm",
		"body": [
			"\\begin{addmargin}[${1:1em}]{${2:0em}}",
			"\t${3:$TM_SELECTED_TEXT}",
			"\\end{addmargin}"
		]
	},
	"[G] Math in-line": {
		"prefix": "mi",
		"body": [
			"\\(${1:$TM_SELECTED_TEXT}\\)"
		]
	},
	"[G] Under-brace": {
		"prefix": "unbc",
		"body": [
			"\\underbrace{${1:$TM_SELECTED_TEXT}}_{$2}"
		]
	},
	"[G] Over-brace": {
		"prefix": "ovbc",
		"body": [
			"\\overbrace{${1:$TM_SELECTED_TEXT}}_{$2}"
		]
	},
	"[G] Bold": {
		"prefix": "bld",
		"body": [
			"\\textbf{${1:$TM_SELECTED_TEXT}}"
		]
	},
	"[G] Italic": {
		"prefix": "itl",
		"body": [
			"\\textit{${1:$TM_SELECTED_TEXT}}"
		]
	},
	"[G] Chemistry equation": {
		"prefix": "ce",
		"body": [
			"\\ce{${1:$TM_SELECTED_TEXT}}"
		]
	},
	"[G] Chapter": {
		"prefix": "chp",
		"body": [
			"\\chapter{${1:$TM_SELECTED_TEXT}}",
			"",
			"$0"
		]
	},
	"[G] Section": {
		"prefix": "sec",
		"body": [
			"\\section{${1:$TM_SELECTED_TEXT}}",
			"",
			"$0"
		]
	},
	"[G] Subsection": {
		"prefix": "ssec",
		"body": [
			"\\subsection{${1:$TM_SELECTED_TEXT}}",
			"",
			"$0"
		]
	},
	"[G] Subsubsection": {
		"prefix": "sssec",
		"body": [
			"\\subsubsection{${1:$TM_SELECTED_TEXT}}",
			"",
			"$0"
		]
	},
	"[G] Example": {
		"prefix": "sssec*",
		"body": [
			"\\subsubsection*{${1:$TM_SELECTED_TEXT}}",
			"",
			"$0"
		]
	},
	"[G] Text": {
		"prefix": "txt",
		"body": [
			"\\text{${1:$TM_SELECTED_TEXT}}"
		]
	},
	"[G] Minted block": {
		"prefix": "mitb",
		"body": [
			"\\begin{minted}{$1}",
			"${2:$TM_SELECTED_TEXT}",
			"\\end{minted}"
		]
	},
	"[G] Minted in-line": {
		"prefix": "miti",
		"body": [
			"\\mintinline{$1}{${2:$TM_SELECTED_TEXT}}"
		]
	},
	"[G] Cancel": {
		"prefix": "can",
		"body": [
			"\\cancel{${1:$TM_SELECTED_TEXT}}"
		]
	}
}